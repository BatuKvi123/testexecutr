-- StarterGui.Main.Shell.execute.LocalScript
local function C_6()
local script = G2L["6"];
	script.Parent.MouseButton1Down:Connect(function()
		if _G.Attached == true then
			loadstring(script.Parent.Parent.Input.Text)
		else
			script.Parent.Parent.title.Text = "Fase Executor(Please Attach!)"
			wait(0.2)
			script.Parent.Parent.title.Text = "Fase Executor"
		end
	end)
end;
task.spawn(C_6);
-- StarterGui.Main.Shell.Clear.LocalScript
local function C_8()
local script = G2L["8"];
	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Input.Text = ""
	end)
end;
task.spawn(C_8);
-- StarterGui.Main.Shell.copy.LocalScript
local function C_a()
local script = G2L["a"];
	script.Parent.MouseButton1Down:Connect(function()
		setclipboard(script.Parent.Parent.Input.Text)
	end)
end;
task.spawn(C_a);
-- StarterGui.Main.Shell.savetf.LocalScript
local function C_c()
local script = G2L["c"];
	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.STF.Visible = not script.Parent.Parent.STF.Visible
	end)
end;
task.spawn(C_c);
-- StarterGui.Main.Shell.attach.LocalScript
local function C_e()
local script = G2L["e"];
	_G.Attached = false
	
	
	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.title.Text = "Fase Executor(Injecting...)"
		wait(1)
		script.Parent.Parent.title.Text = "Fase Executor(Checking Whitelist...)"
		wait(1)
		script.Parent.Parent.title.Text = "Fase Executor(Done!)"
		wait(1)
		script.Parent.Parent.title.Text = "Fase Executor"
		wait(0.00001)
		_G.Attached = true
	end)
	
end;
task.spawn(C_e);
-- StarterGui.Main.Shell.STF.Done.LocalScript
local function C_14()
local script = G2L["14"];
	script.Parent.MouseButton1Down:Connect(function()
		writefile(script.Parent.Parent.Name.Text, script.Parent.Parent.Parent.Input.Text)
	end)
end;
task.spawn(C_14);
-- StarterGui.Main.Shell.STF.LocalScript
